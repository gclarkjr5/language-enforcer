{
  "productName": "Language Enforcer Review",
  "version": "0.1.0",
  "identifier": "com.languageenforcer.review",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "https://timbered-greedily-alyvia.ngrok-free.dev",
    "frontendDist": "../frontend/dist"
  },
  "app": {
    "windows": [
      {
        "title": "Language Enforcer Review",
        "width": 900,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "devCsp": "default-src 'self' data: blob: ipc: https: wss:; connect-src 'self' ipc: https: wss:; img-src 'self' data: blob: https:; style-src 'self' 'unsafe-inline' https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": {
      "../../data/words.db": "data/words.db",
      "../../data/config.toml": "data/config.toml"
    }
  }
}